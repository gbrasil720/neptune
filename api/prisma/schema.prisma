generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  firstName String
  lastName  String
  email     String
  telephone String
  role      RoleEnum @default(MEMBER)
}

enum RoleEnum {
  ADMIN
  MODERATOR
  SUBSCRIBER
  MEMBER
}
